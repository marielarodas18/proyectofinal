<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cancion" table="Cancion">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cancion" type="string" column="Cancion" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="duracion" type="string" column="Duracion" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="disco" type="string" column="Disco" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
